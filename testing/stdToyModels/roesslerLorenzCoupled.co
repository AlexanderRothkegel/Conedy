network net;

roessler_omega = 20.0;
roessler_a = 0.165;
roessler_b = 0.2;
roessler_c = 10.0;

lorenz_S = 10.0;
lorenz_r = 28.0;
lorenz_b = 8.0/3.0;

samplingTime = 0.01;


net.addNode(roessler());
net.addNode(lorenz());


net.addEdge(1,0,2.5);


net.setState(0, 0.0, 0.0, 0.0);
net.setState(1, 1.0, 1.0, 1.0);


net.observeTime("output/roesslerLorenzCoupled.co.series");

net.observe(0,"output/roesslerLorenzCoupled.co.series",component(0));
net.observe(0,"output/roesslerLorenzCoupled.co.series",component(1));
net.observe(0,"output/roesslerLorenzCoupled.co.series",component(2));

net.observe(1,"output/roesslerLorenzCoupled.co.series",component(0));
net.observe(1,"output/roesslerLorenzCoupled.co.series",component(1));
net.observe(1,"output/roesslerLorenzCoupled.co.series",component(2));


net.evolve(0.0,1500.0);

