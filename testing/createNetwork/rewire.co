

network net;

setRandomSeed(0);

net.cycle ( 1000, 10, roessler(), weightedEdge(1.0));




print "mean degree before rewiring:" + net.meanDegree()+ newline;
print "should be close to 0.75:" +  net.meanClustering()+ newline;
net.rewire(1.0);
net.saveAdjacencyList("testConedy");
print "mean degree after rewiring:" +  net.meanDegree()+ newline;
print "should be close to 0.0:" +  net.meanClustering()+ newline;





